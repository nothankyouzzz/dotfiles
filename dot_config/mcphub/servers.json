{
  "mcpServers": {
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "command": "npx",
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "6000"
      }
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx",
      "disabled": true
    },
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "~/sync:~/.config/nvim"
      ],
      "command": "npx",
      "disabled": true
    },
    "git": {
      "args": [
        "mcp-server-git",
        "--repository",
        "${REPOSITORY_PATH}"
      ],
      "command": "uvx",
      "disabled": true
    },
    "github": {
      "disabled": true,
      "headers": {
        "Authorization": "Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "command": "npx"
    },
    "playwright": {
      "args": [
        "@playwright/mcp@latest"
      ],
      "command": "npx",
      "disabled": false
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx"
    },
    "time": {
      "args": [
        "mcp-server-time"
      ],
      "command": "uvx",
      "disabled": true
    },
    "vectorcode": {
      "command": "vectorcode-mcp-server",
      "disabled": true
    }
  }
}
